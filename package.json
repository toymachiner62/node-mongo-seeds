{
  "name": "node-mongo-seeds",
  "version": "3.1.0",
  "description": "A tool to populate a mongo db from json files",
  "repository": {
    "type": "git",
    "url": "https://github.com/toymachiner62/node-mongo-seeds"
  },
  "keywords": [
    "node",
    "mongo",
    "seed"
  ],
  "author": "Tom Caflisch <tomcaflisch@gmail.com>",
  "main": "./lib/seed.js",
  "bin": {
    "seed": "./bin/seed",
    "seed-setup": "./bin/setup"
  },
  "scripts": {
